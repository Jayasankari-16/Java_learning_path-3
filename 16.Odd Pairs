/*Given an integer N, determine the number of pairs (A,B) such that: 1≤A,B≤N; A+B is odd.*/

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
		int t=sc.nextInt();
		while(t-->0){
		    long n=sc.nextLong();
		    System.out.println(n*n/2);
		}
	}
}
